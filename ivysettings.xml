<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
<settings defaultResolver="chain-resolver" />
<resolvers>
   <chain name="chain-resolver">
      <filesystem name="mylocal">
         <ivy pattern= "D:/libs/[organisation]/[module]/type]s/artifact]-[revision]-[type]s.[ext]" />
         <artifact pattern= "D:/libs/[organisation]/[module]/type]s/artifact]-[revision]-[type]s.[ext]" />
      </filesystem>
      <ibiblio name="ibiblio" m2compatible="true" />
   </chain>
</resolvers>
</ivysettings>